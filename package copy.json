    "@testing-library/preact": "3.2.3",
    "@testing-library/preact-hooks": "1.1.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/chrome": "0.0.254",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.6",
